@model IEnumerable<SistemaReservasViajes.Models.Auto>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.ReservaID)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.DestinoID)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Marca)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Modelo)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.FechaRecogida)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.FechaDevolucion)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.ReservaID)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.DestinoID)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Marca)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Modelo)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.FechaRecogida)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.FechaDevolucion)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.AutoID">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.AutoID">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.AutoID">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
